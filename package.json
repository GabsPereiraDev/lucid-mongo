{
  "name": "@zakodium/lucid-mongo",
  "description": "A Mongodb ODM",
  "main": "index.js",
  "version": "3.1.5",
  "scripts": {
    "pretest": "npm run test:cleanup && npm run lint",
    "test:cleanup": "rm -rf test/functional/database && rm -rf test/unit/tmp",
    "posttest": "npm run test:cleanup && npm run coverage",
    "test:local": "npm run test:cleanup && FORCE_COLOR=true DB=mongodb node japaFile.js",
    "test": "npm run test:local",
    "test:win": "set DB=mongodb&& node japaFile.js",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 1",
    "commit": "git-cz"
  },
  "keywords": [
    "adonis",
    "lucid",
    "mongo",
    "mongodb",
    "orm",
    "odm"
  ],
  "author": "LuongND",
  "license": "MIT",
  "dependencies": {
    "@adonisjs/generic-exceptions": "^3.0.1",
    "chance": "^1.0.18",
    "debug": "^4.1.1",
    "geo-point": "^1.2.0",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "mongo-uri-builder": "^2.0.0",
    "mongodb": "^3.2.6",
    "mongodb-url": "^3.0.2",
    "mquery": "^3.2.0",
    "pluralize": "^8.0.0",
    "pretty-hrtime": "^1.0.3",
    "require-all": "^3.0.0"
  },
  "devDependencies": {
    "@adonisjs/ace": "^5.0.8",
    "@adonisjs/fold": "^4.0.9",
    "@adonisjs/sink": "^1.0.17",
    "clear-require": "^3.0.0",
    "conventional-changelog-cli": "^2.0.21",
    "cz-conventional-changelog": "^2.1.0",
    "fs-extra": "^8.0.1",
    "japa": "^2.0.10",
    "japa-cli": "^1.0.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zakodium/lucid-mongo.git"
  }
}
